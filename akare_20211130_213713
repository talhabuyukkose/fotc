--
-- Name: getnotification_withemail; Type: VIEW; Schema: public; Owner: akare
--

CREATE VIEW public.getnotification_withemail AS
 SELECT ma.id,
    us.emailaddress AS frommail,
    us2.emailaddress AS tomail,
    ma.message,
    ma.createdate,
    ma.isitseen
   FROM ((public.mailnotification ma
     LEFT JOIN public."user" us ON ((us.id = ma.fromuserid)))
     LEFT JOIN public."user" us2 ON ((us2.id = ma.touserid)))
  ORDER BY ma.id DESC;


ALTER TABLE public.getnotification_withemail OWNER TO akare;

