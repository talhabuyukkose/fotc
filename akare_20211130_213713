CREATE PROCEDURE public.addcategory(userid integer, parentid integer, categoryname character varying, categoryparentname character varying, categorypath character varying, categoryparentpath character varying)
    LANGUAGE sql
    AS $$
INSERT INTO public.category(
	userid, parentid, categoryname, categoryparentname, createdate, categorypath, categoryparentpath,isdelete)
	VALUES ("userid", "parentid", "categoryname", "categoryparentname",timezone('turkey', now()), "categorypath", "categoryparentpath",false);

$$;


