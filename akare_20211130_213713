--
-- Name: addnotification(character varying, character varying, character varying, integer); Type: PROCEDURE; Schema: public; Owner: akare
--

CREATE PROCEDURE public.addnotification(fromemail character varying, toemail character varying, message character varying, replyid integer)
    LANGUAGE sql
    AS $$
INSERT INTO public.mailnotification (fromuserid
									 , message
									 , createdate
									 , isitseen
									 , isdelete
									 , touserid
									 , sent
									 ,replyid)
                    VALUES ((select id from public.user where emailaddress="fromemail")
							,"message"
							,timezone('turkey', now())
							, false
							, false
							, (select id from public.user where emailaddress="toemail")
							,true
						   ,"replyid");
					
					
$$;


ALTER PROCEDURE public.addnotification(fromemail character varying, toemail character varying, message character varying, replyid integer) OWNER TO akare;

