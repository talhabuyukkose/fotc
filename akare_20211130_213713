CREATE PROCEDURE public.updateuser(id integer, name character varying, surname character varying, title character varying, password character varying, department character varying, emailaddress character varying)
    LANGUAGE sql
    AS $$
UPDATE public.user
	SET name ="name", surname = "surname", title = "title", password = "password", department = "department", emailaddress =  "emailaddress"
	WHERE  id="id"
				
$$;


